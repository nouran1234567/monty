#include <stdio.h>
#include <stdlib.h>

/**
 * push - push elements to the stack
 *
 * Return: no return
 */
 
#define SZ_STA 100

int stack[SZ_STA];
int prnt_sta = 0;

void push(int value) 
/* checks if the stack is full if yes print an error message and exit the program */
{
    if (prnt_sta >= SZ_STA) 
    {
        printf("L%d: stack overflow\n", __LINE__);
        exit(EXIT_FAILURE);
    }
/* if no push the value to stack & increments the stack pointer */
    stack[prnt_sta++] = value;
}

/**
 * pall - prints elements on the stack
 *
 * Return: no return
 */

void pall() 
/*The function first checks if the stack is empty if yes return*/ 
{
    if (prnt_sta == 0) 
    {
        return;
    }
/*if no go over the stack in reverse order and prints each value*/
    for (int z = prnt_sta - 1; z >= 0; z--) 
    {
        printf("%d\n", stack[z]);
    }
}

int main() 
{
  int x;

  scanf("%d", &x);

  push(x);

  pall();

  return 0;
}
